# Run -V- Kernel X main script file
# Written for -V- Kernel X by Vagelis1608 @xda-developers

on post-fs-data
    start vkx

    start sinit

service vkx /sbin/vkx-script.sh
    user root
    group root
    seclabel u:r:su:s0
    oneshot
    disabled

service sinit /system/bin/sysinit
    user root
    group root
    seclabel u:r:su:s0
    oneshot
    disabled

